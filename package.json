{
  "name": "my-razzle-app",
  "version": "2.0.0-alpha.2",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "format": "prettier-standard 'src/**/*.js'"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest.beforeload.js"
    ]
  },
  "dependencies": {
    "arboris": "^1.0.3",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "axios-retry": "^3.1.1",
    "express": "^4.16.3",
    "humps": "^2.0.1",
    "lodash": "^4.17.11",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "mobx-state-tree": "^3.5.0",
    "raw-loader": "^0.5.1",
    "razzle": "^2.4.0",
    "razzle-heroku": "https://github.com/stokestudio/razzle-heroku.git",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "sinon": "^6.3.4",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0",
    "prettier-standard": "^8.0.1"
  }
}
